<Window x:Class="TotalCommander.Windows.DeleteAsk"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TotalCommander.Windows"
        mc:Ignorable="d"
        Title="DeleteAsk" Height="100" Width="300
        ">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Czy chcesz usunąć:" VerticalAlignment="Top" Width="102"/>
        <TextBlock x:Name="NameBlock" HorizontalAlignment="Left" Margin="112,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="170"/>
        <Button x:Name="NoButton" Content="Nie" HorizontalAlignment="Left" Margin="207,39,0,0" VerticalAlignment="Top" Width="75" Click="NoButton_Click"/>
        <Button x:Name="YesButton" Content="Tak" HorizontalAlignment="Left" Margin="127,39,0,0" VerticalAlignment="Top" Width="75" Click="YesButton_Click"/>

    </Grid>
</Window>
